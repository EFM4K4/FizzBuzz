#create for loop to print 1-100
for i in range(1, 101):
#create condition where numbers divisible by 3 and 5 to print FizzBuzz
    if i% 3 == 0 and i% 5 == 0:
        print (str(i) + "FizzBuzz")
        
#create condition where numbers divisible by 3 to print Fizz
    elif i% 3 ==0:
        print (str(i)+"Fizz")
        
#create condition where numbers divisible by 5 to print Buzz
    elif i% 5 == 0:
        print(str(i) + "Buzz")
        
#Print out numbers 1-100
    else:
        print(str(i)) 
